{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ServerOptions": {
    "AddServerHeader": false
  },
  "ReverseProxy": {
    "ConnectionTimeout": "00:00:01.000",
    "Listen": {
      "http": {
        "Protocols": [ "Http1" ],
        "Address": [ "127.0.0.1:5001", "[::1]:5001" ]
      }
    },
    "Routes": {
      "Match": {
        "Hosts": [ "*" ],
        "Paths": [ "/ai/*" ]
      },
      "ClusterId": "apidemo",
      "Timeout": "00:10:00",
      "Metadata": {
        "AiMapping": "{\"openai\":{},\"deepseek\":{}}"
      }
    },
    "Clusters": {
      "deepseek": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": [
          {
            "Address": "https://api.deepseek.com/v1/chat/completions"
          }
        ]
      },
      "openai": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": [
          {
            "Address": "https://api.openai.com/v1/chat/completions"
          }
        ]
      }
    }
  }
}