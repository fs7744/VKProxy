{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "ServerOptions": {
    "AddServerHeader": false
  },
  "ReverseProxy": {
    "Listen": {
      "http": {
        "Protocols": [ "Http1", "Http2", "Http3" ],
        "Address": [ "*:5001" ],
        "UseSni": true,
        "CheckCertificateRevocation": false,
        "SniId": "localhost"
      }
    },
    "Sni": {
      "localhost": {
        "Host": [ "*" ],
        "Tls": {
          "Path": "testCert.pfx",
          "Password": "testPassword"
        }
      }
    }
  }
}